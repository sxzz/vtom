  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/vtom#readme",
  "bugs": {
    "url": "https://github.com/sxzz/vtom/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/vtom.git"
  },
